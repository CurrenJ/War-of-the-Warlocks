package Entities;

import java.util.ArrayList;

public class Entity {
	ArrayList<String> names;
	ArrayList<Double> speeds;
	ArrayList<Double> angles;
	
	double xPos;
	double yPos;
	
	public Entity(int x, int y){
		names = new ArrayList();
		speeds = new ArrayList();
		angles = new ArrayList();
	
		xPos = x;
		yPos = y;
	}
	
	
}
